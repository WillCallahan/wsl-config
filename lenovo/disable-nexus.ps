Write-Host 'Disabling nexus configuration...' -ForegroundColor yellow

# Npm Configuration
npm config set registry https://registry.npmjs.org/
npm set strict-ssl false

yarn config delete registry
yarn set strict-ssl false

# Python Pip Configuration
pip config unset global.index
pip config unset global.index-url
pip config unset global.trusted-host

# Nuget Configuration
dotnet nuget enable source nuget.org
dotnet nuget remove source nexus.i.callahanwilliam.com

Write-Host 'Nexus configuration disabled' -ForegroundColor yellow
